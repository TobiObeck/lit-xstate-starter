
{
    "compilerOptions": {
        "target": "es6",
        "module": "esnext",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "checkJs": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false
    },
    "include": [
        "src/**/*"
    ]
}    
